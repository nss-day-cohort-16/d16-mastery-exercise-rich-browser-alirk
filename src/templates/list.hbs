{{> nav}}

{{#each toys}}

<div class="col-sm-4">
	<ul>
		<li><img src='{{img}}'></li>
		<li><h4>{{name}}</h4></li>
		<li>{{price}}</li>
		<li>{{description}}</li>
	</ul>

	<button class="detailsBtn" id="{{fbKey}}">View details</button>

	<button class="deleteBtn" id="{{fbKey}}">Delete toy</button>

</div>



{{/each}}